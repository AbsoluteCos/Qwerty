<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.sphinx.Alert">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Alert!" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <TitledPane text="Stack Trace">
               <content>
                  <ScrollPane fitToHeight="true" fitToWidth="true" prefViewportHeight="400.0" prefViewportWidth="600.0" BorderPane.alignment="CENTER">
                     <content>
                        <TextArea fx:id="stackTraceArea" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                     </content>
                  </ScrollPane>
               </content>
            </TitledPane>
         </bottom>
         <center>
            <Text fx:id="messageText" strokeType="OUTSIDE" strokeWidth="0.0" text="[message]" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
